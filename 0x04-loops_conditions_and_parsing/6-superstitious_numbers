#!/usr/bin/env bash
# create a script that displays numbers from 1 to 20
# create a script that uses case statements
x=1
while [ $x -le 20 ]
do
    echo "$x"
    case $x in
	4)
	    echo "bad luck from China"
	    ;;
	9)
	    echo "bad luck from Japan"
	    ;;
	17)
	    echo "bad luck from Italy"
	    ;;
    esac
    ((x++))
done
